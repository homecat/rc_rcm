.onLoad {
	background:url(../images/onLoad.gif) no-repeat 3px 3px;
	padding-left:25px;
	font-size: 12px;
	min-height:15px;
	vertical-align:middle;
}
.onCorrect {
	background:url(../images/reg-menu.gif) no-repeat 0px -16px;
	padding:5px 10px;
	min-height:15px;
	margin-left:10px;
}
.onError .onFocus p {
	list-style-type:none;
	list-style-image:none;
	line-height : 2;
}
.onError .onFocus span {
	font-size : 12px;
}
.onFocus {
	width:287px;
	height:20px;
	padding-top:3px;
	left:0px;
	top:0px;
	padding:0px;
	margin:0px;
}
.onFocus_top {
	padding:2px 10px 5px 28px;
	min-height:15px;
	/*display:block;*/
	background:url(../images/verification-ui7.gif) no-repeat -4px -2px;
}
.onFocus_bot {
	width:287px;
	height:5px;
	overflow:hidden;/*display:block;
	background:url(../images/verification-ui6.gif) no-repeat 0px -185px;*/
}
.onError {
	width:287px;
	height:20px;
	padding-top:3px;
	left:0px;
	top:0px;
	padding:0px;
	margin:0px;
}
.onError_top {
	padding:2px 10px 5px 28px;
	min-height:15px;
	_height:10px;
	/*display:block;*/
	background:url(../images/verification-ui7.gif) no-repeat -4px -192px;
}
.onError_bot {
	width:287px;
	height:5px;
	/*display:block;
	background:url(../images/verification-ui6.gif) no-repeat 0px -375px; */
	overflow:hidden;
}

